{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Ruleset for material is 'cell line'.",
  "title": "FAANG sample metadata rules for 'cell line'",
  "name": "faang_samples_cell_line",
  "$async": true,
  "type": "object",
  "required": ["organism", "sex", "cell_line", "biomaterial_provider"],
  "properties": {
    "describedBy": {
      "const": "https://github.com/FAANG/faang-metadata/blob/master/docs/faang_sample_metadata.md"
    },
    "schema_version": {
      "description": "The version number of the schema in major.minor.patch format.",
      "type": "string",
      "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
      "example": "4.6.1"
    },
    "samples_core": {
      "description": "Core samples-level information",
      "type": "object",
      "$ref": "core/samples/faang_samples_core.metadata_rules.json"
    },
    "organism": {
      "type": "object",
      "name": "organism",
      "description": "NCBI taxon ID of organism.",
      "required": ["term", "text"],
      "properties": {
        "term": {
          "type": "string",
          "graph_restriction": {
            "ontologies": ["obo:ncbitaxon"],
            "classes": ["NCBITaxon:1"],
            "relations": ["rdfs:subClassOf"],
            "direct": false,
            "include_self": false
          }
        },
        "text": {
          "type": "string"
        },
        "mandatory": {
          "const": "mandatory"
        },
        "ontology_name": {
          "const": "NCBITaxon"
        }
      }
    },
    "sex": {
      "type": "object",
      "name": "sex",
      "description": "Animal sex, described using any child term of PATO_0000047.",
      "required": ["term", "text"],
      "properties": {
        "term": {
          "type": "string",
          "graph_restriction": {
            "ontologies": ["obo:pato"],
            "classes": ["PATO:0000047"],
            "relations": ["rdfs:subClassOf"],
            "direct": false,
            "include_self": false
          }
        },
        "text": {
          "type": "string"
        },
        "mandatory": {
          "const": "mandatory"
        },
        "ontology_name": {
          "const": "PATO"
        }
      }
    },
    "cell_line": {
      "type": "object",
      "name": "cell line",
      "description": "Name of the cell line.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "mandatory"
        }
      }
    },
    "biomaterial_provider": {
      "type": "object",
      "name": "biomaterial provider",
      "description": "Name of company or lab that supplied the cell line.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "mandatory"
        }
      }
    },
    "catalogue_number": {
      "type": "object",
      "name": "catalogue number",
      "description": "Identifier for the cell line in the suppliers catalogue. E.g. 'ACC 701' for IPEC-J2 from DSMZ.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "recommended"
        }
      }
    },
    "number_of_passages": {
      "type": "object",
      "name": "number of passages",
      "description": "The number of times the cell line has been re-plated and allowed to grow back to confluency or to some maximum density if using suspension cultures.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "number"
        },
        "mandatory": {
          "const": "recommended"
        }
      }
    },
    "date_established": {
      "type": "object",
      "name": "date established",
      "description": "Date the line was established/re-established.",
      "required": ["value", "units"],
      "properties": {
        "value": {
          "type": "string",
          "pattern": "^[12]\\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01])|[12]\\d{3}-(0[1-9]|1[0-2])|[12]\\d{3}$"
        },
        "mandatory": {
          "const": "recommended"
        },
        "units": {
          "type": "string",
          "enum": ["YYYY-MM-DD", "YYYY-MM", "YYYY"]
        }
      }
    },
    "publication": {
      "type": "object",
      "name": "publication",
      "description": "Publication Digitial Object Identifier (DOI) e.g. 'doi://10.1139/o09-005'. Publication where the cell line has been fully described and should include details such as doubling time and adhesion preference. You can use pubmed to find your DOI https://www.ncbi.nlm.nih.gov/pubmed.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "recommended"
        }
      }
    },
    "breed": {
      "type": "object",
      "name": "breed",
      "description": "Animal breed, described using the FAANG breed description guidelines(http://bit.ly/FAANGbreed).",
      "required": ["term", "text"],
      "properties": {
        "term": {
          "type": "string",
          "graph_restriction": {
            "ontologies": ["obo:lbo"],
            "classes": ["LBO:0000000"],
            "relations": ["rdfs:subClassOf"],
            "direct": false,
            "include_self": false
          }
        },
        "text": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        },
        "ontology_name": {
          "const": "LBO"
        }
      }
    },
    "cell_type": {
      "type": "object",
      "name": "cell type",
      "description": "Cell type using a child term of either CL_0000000 or BTO_0000000.",
      "required": ["term", "text"],
      "properties": {
        "term": {
          "type": "string",
          "graph_restriction": {
            "ontologies": ["obo:cl", "obo:bto"],
            "classes": ["CL:0000000", "BTO:0000000"],
            "relations": ["rdfs:subClassOf"],
            "direct": false,
            "include_self": false
          }
        },
        "text": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        },
        "ontology_name": {
          "type": "string",
          "enum": ["CL", "BTO"]
        }
      }
    },
    "culture_conditions": {
      "type": "object",
      "name": "culture conditions",
      "description": "Brief description of culture conditions (e.g. 'on feeder cells', 'E8 media').",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        }
      }
    },
    "culture_protocol": {
      "type": "object",
      "name": "culture protocol",
      "description": "Link to protocol describing the maintenance of the culture.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string",
          "format": "uri"
        },
        "mandatory": {
          "const": "optional"
        }
      }
    },
    "disease": {
      "type": "object",
      "name": "disease",
      "description": "Disease child term of either PATO_0000461 or EFO_0000408.",
      "required": ["term", "text"],
      "properties": {
        "term": {
          "type": "string",
          "graph_restriction": {
            "ontologies": ["obo:pato", "obo:efo"],
            "classes": ["PATO:0000461", "EFO:0000408"],
            "relations": ["rdfs:subClassOf"],
            "direct": false,
            "include_self": false
          }
        },
        "text": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        },
        "ontology_name": {
          "type": "string",
          "enum": ["PATO", "EFO"]
        }
      }
    },
    "karyotype": {
      "type": "object",
      "name": "karyotype",
      "description": "Karyotype of the cell line.",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        }
      }
    },
    "derived_from": {
      "type": "object",
      "name": "Derived from",
      "description": "Sample name or BioSample ID for the sample or animal the cell line was derived from, where this is known and can be described within the FAANG standards (optional).",
      "required": ["value"],
      "properties": {
        "value": {
          "type": "string"
        },
        "mandatory": {
          "const": "optional"
        }
      }
    }
  }
}